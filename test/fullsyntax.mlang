#include <math.mlang>
#include <raylib.mlang>

// io is a module
#import std::io from <std.mlang>

/*
	multi line comment
*/

int width = 300;

module game;
func game::init() -> bool {
	// function calls
	rl::init_window(width, 1000, "window name");
	return true;
}
func game::close(){
	rl::close_window();
	return true;
}

// entry point
func main() -> int{
    const bool closed = false;
	// unused return
	game::init();

	if (!game::close()){
		io::println("game couldn't close");
	}
    return 0;
}
