#include <c_std.mlang>

func add(int a, int b, int c) -> int {
    return a + b + c;
}
func shift_args(int* argc, string** argv) -> string {
    string ret = **argv;
    *argc -= 1;
    *argv += 8;
    return ret;
}

func main(int argc, string* argv) {
    if(1)
        c_std::printf("program_name: %s\n", shift_args(&argc, &argv));
}
