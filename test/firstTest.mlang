#include <c_std.mlang>

struct Vec3 {
    float x = 0.0; 
    float y = 0.0;
    float z = 0.0;
    func add(float amount) {
        this.x += amount;
        this.y += amount;
        this.z += amount;
    }
    func init() {
        this.x = 0.0;
        this.y = 0.0;
        this.z = 0.0;
    }
}
func static Vec3.new() -> Vec3* {
    return c_std::malloc(Vec3.type_info().size);
}
func Vec3.print() {
    c_std::printf("Vec3[%.2f, %.2f, %.2f]\n", this.x, this.y, this.z);
}

const ONE = 1;
func Vec3.add_vec(Vec3 v) -> Vec3 {
    v.x += this.x;
    v.y += this.y;
    v.z += this.z;
    return v;
}
