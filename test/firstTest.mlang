#include <c_std.mlang>

func shift_args(int* argc, string** argv) -> string {
    string ret = **argv;
    *argc -= 1;
    *argv += 8;
    return ret;
}
func fibo(int n) -> int {
    if (n == 0) return 0;
    if (n == 1) return 1;
    return fibo(n-1) + fibo(n-2);
}

func loop (int n){
    c_std::printf("%d -> %d\n", n, fibo(n));

    n += 1;
    if (n <= 30)
        loop(n);
}
func main(int argc, string* argv) {
    loop(0);
    if(argc > 0) {
        c_std::printf("%s\n", shift_args(&argc, &argv));
    }
}
