#include <c_std.mlang>

func shift_args(int* argc, string** argv) -> string {
    string ret = **argv;
    *argc -= 1;
    *argv += 8;
    return ret;
}
func fibo(int n) -> int {
    if (n == 0) return 0;
    if (n == 1) return 1;
    int last;
    last = fibo(n-1);
    return (last + fibo(n-2));
}

func main(int argc, string* argv) {
    c_std::printf("fibo 12: %d\n", fibo(1));
    c_std::printf("fibo 12: %d\n", fibo(2));
    c_std::printf("fibo 12: %d\n", fibo(3));
    c_std::printf("fibo 12: %d\n", fibo(4));
    c_std::printf("fibo 12: %d\n", fibo(5));
    c_std::printf("fibo 12: %d\n", fibo(6));
    if(argc > 0) {
        c_std::printf("%s\n", shift_args(&argc, &argv));
    }
}
