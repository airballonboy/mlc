struct Vec3 {
    int x = 0; 
    int y = 0;
    int z = 0;
    func init() {
        this.x = 0;
        this.y = 0;
        this.z = 0;
    }
    func print() {
        c_std::printf("[%d, %d, %d]\n", this.x, this.y, this.z);
    }
}

const ONE = 1;

func main(int argc, string* argv) {
    Vec3 v;
    v.x = ONE;
    v.print();
}
