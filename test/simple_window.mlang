#include <raylib.mlang>


func main() {
    rl::init_window(1200, 675, "hello raylib");
    rl::Color c;
    c = rl::get_color(0x181818ff);
    while (!rl::window_should_close()) {
        rl::begin_drawing();

        rl::clear_background(c);

        rl::end_drawing();
    }
    rl::close_window();
}
